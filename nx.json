{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "container",
          "helm-package",
          "terraform-fmt",
          "terraform-init",
          "terraform-validate",
          "terraform-plan",
          "terraform-apply"
        ]
      }
    }
  },
  "targetDefaults": {
    "test": {
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/jest.preset.js"
      ]
    },
    "lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json", "helm"]
    },
    "version": {
      "dependsOn": ["build"]
    },
    "container": {
      "dependsOn": ["build"],
      "inputs": ["container"]
    },
    "helm-package": {
      "inputs": ["helm"],
      "outputs": ["{workspaceRoot}/libs/next-helm-chart/dist/*.tgz"]
    },
    "helm-push": {
      "dependsOn": ["helm-package"],
      "inputs": ["helm"]
    },
    "terraform-fmt": {
      "inputs": ["terraform"]
    },
    "terraform-init": {
      "inputs": ["terraform"],
      "outputs": [
        "{projectRoot}/deploy/terraform/.terraform",
        "{projectRoot}/deploy/terraform/.terraform.lock.hcl"
      ]
    },
    "terraform-validate": {
      "dependsOn": ["terraform-init"],
      "inputs": ["terraform"]
    },
    "terraform-plan": {
      "inputs": ["terraform"],
      "outputs": ["{projectRoot}/deploy/terraform/*.tfplan"]
    },
    "terraform-apply": {
      "inputs": ["terraform"]
    }
  },
  "affected": {
    "defaultBase": "master"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "css",
        "linter": "eslint"
      }
    },
    "@ensono-stacks/workspace": {
      "init": {
        "pipelineRunner": "taskctl"
      }
    }
  },
  "stacks": {
    "config": {
      "business": {
        "company": "amido",
        "domain": "stacks",
        "component": "core"
      },
      "domain": {
        "internal": "nonprod.amidostacks.com",
        "external": "prod.amidostacks.com"
      },
      "cloud": {
        "platform": "azure",
        "region": "euw",
        "group": "amido-stacks-nonprod-euw-core"
      },
      "pipeline": "azdo",
      "terraform": {
        "group": "Stacks-Ancillary-Resources",
        "storage": "amidostackstfstate",
        "container": "tfstate"
      },
      "vcs": {
        "type": "github",
        "url": "https://github.com/amido/cycle-11-multi-app-test.git"
      }
    },
    "executedGenerators": {
      "project": {
        "next-cypress-application": [
          "CypressInit",
          "NextInit",
          "NextInitDeployment"
        ]
      },
      "workspace": [
        "WorkspaceInit",
        "WorkspaceDeployment",
        "CypressInitDeployment"
      ]
    }
  },
  "extends": "nx/presets/npm.json",
  "namedInputs": {
    "default": ["!{projectRoot}/Dockerfile"],
    "container": ["{projectRoot}/Dockerfile"],
    "helm": ["{workspaceRoot}/libs/next-helm-chart/build/helm/**/*"],
    "terraform": [
      "{projectRoot}/deploy/terraform/*.tf",
      "{projectRoot}/deploy/terraform/*.tfvars",
      "{projectRoot}/deploy/terraform/.terraform.lock.hcl"
    ]
  }
}
